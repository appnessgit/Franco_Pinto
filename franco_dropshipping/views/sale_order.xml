<odoo>
  <data>
    <record id="sale_order_inherited_form_purchase_dropship" model="ir.ui.view">
      <field name="name">sale.order.inherited.form.purchase.dropship</field>
      <field name="model">sale.order</field>
      <field name="inherit_id" ref="sale.view_order_form"/>
      <field name="arch" type="xml">
          <xpath expr="//button[@name='action_confirm']" position="after">
                <button name="generate_rfq" string="Generate RFQ" type="object" class="oe_highlight"/>
          </xpath>
      </field>
  </record>
  </data>
</odoo>